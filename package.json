		"styled-components": "4.3.2"
	},
	"devDependencies": {
		"@types/styled-components": "4.1.18",
		"babel-core": "^6.26.3",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"parcel-bundler": "1.12.3",
